<UserControl x:Class="PP.Wpf.Demo.Views.LoadingView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:pp="https://www.cnblogs.com/pumbaa"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Background="White" FontSize="15">
    <WrapPanel Margin="10">
        <pp:ProgressRingWin10 Margin="10" Width="100" Foreground="{DynamicResource PP.Brushes.Themes}" />
        <pp:ProgressRing3Color Margin="10" Width="100" />
        <pp:ProgressRingArc1 Margin="10" Width="100" />
        <pp:ProgressRingArc2 Margin="10" Width="100" />
    </WrapPanel>
</UserControl>
